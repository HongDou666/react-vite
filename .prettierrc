{
  "tabWidth": 2,
  "singleQuote": true,
  "semi": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "vueIndentScriptAndStyle": false,
  "printWidth": 120,
  "endOfLine": "auto",
  "useTabs": false,
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "strict"
}
