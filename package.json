{
  "name": "react18-vite",
  "description": "react18+vite5+ts",
  "author": "zqc-wf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "test": "vite --mode test",
    "staging": "vite --mode staging",
    "production": "vite --mode production",
    "devEnv": "cross-env NODE_ENV=development vite --mode development",
    "testEnv": "cross-env NODE_ENV=test vite --mode test",
    "stagingEnv": "cross-env NODE_ENV=staging vite --mode staging",
    "productionEnv": "cross-env NODE_ENV=production vite --mode production",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && vite build --mode development",
    "build:test": "tsc -b && vite build --mode test",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:production": "tsc -b && vite build --mode production",
    "build:devEnv": "tsc -b && cross-env NODE_ENV=development vite build --mode development",
    "build:testEnv": "tsc -b && cross-env NODE_ENV=test vite build --mode test",
    "build:stagingEnv": "tsc -b && cross-env NODE_ENV=staging vite build --mode staging",
    "build:productionEnv": "tsc -b && cross-env NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "prepare": "husky install",
    "check:eslint": "eslint --max-warnings=0 \"{src,mock}/**/*.{ts,tsx}\" --fix",
    "check:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "check:stylelint": "stylelint --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "pre-check": "tsc && npx lint-staged",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "json-server": "json-server --watch ./json-server/db.json --port 3000"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
